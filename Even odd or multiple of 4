number=int(input("Enter a number: "))

class Evenorodd():

    def __init__(self,number):
        self.number=number

    def __str__(self):

        if self.number%4==0:
            print("This number is a Multple of 4")
        elif self.number%2==0:
            print("This is even")

        else:
            print("This is odd")
        return "The number that you have Entered was :{}".format(self.number)




t=Evenorodd(number)
print(t.__str__())
